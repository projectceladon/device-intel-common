            <BooleanParameter Name="bt" Mapping="Control:%1 %2 %3 bt_in"/>
            <!-- On bytcr-rt564x-common, there is no TDM between the SST and the Codec, so we can only use codec0 -->
            <BooleanParameter Name="codec_in" Mapping="Control:%1 %2 %3 codec_in0"/>
