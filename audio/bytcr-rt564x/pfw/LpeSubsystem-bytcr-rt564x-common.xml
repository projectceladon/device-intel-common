            <!--            Description="Voice Auto Detection pipe"/> -->
        </ComponentType>

        <!-- Mixers definition -->
        <ComponentType Name="MainMixerIn" Mapping="Amend2:mix">
            <BooleanParameter Name="pcm0" Mapping="Control:%1 %2 %3 pcm0_in"/>
