        <ComponentType Name="MediaMixerIn" Mapping="Amend2:mix">
            <BooleanParameter Name="compressed" Mapping="Control:%1 %2 %3 media0_in"/>
            <BooleanParameter Name="regular" Mapping="Control:%1 %2 %3 media1_in"/>
            <BooleanParameter Name="media2_in" Mapping="Control:%1 %2 %3 media2_in"/>
            <BooleanParameter Name="deep" Mapping="Control:%1 %2 %3 media3_in"/>
        </ComponentType>

        <ComponentType Name="MainMixerOut">
            <Component Name="mix" Type="MainMixerIn" Mapping="Amend3:0"/>
        </ComponentType>

        <ComponentType Name="MediaMixerOut">
            <Component Name="mix" Type="MediaMixerIn" Mapping="Amend3:0"/>
        </ComponentType>

        <!-- Main Mixer -->
        <ComponentType Name="MainMixerMatrix">
            <!-- <Component Name="media_loop1" Type="MainMixerOut" Mapping="Amend1:media_loop1_out"/> -->
